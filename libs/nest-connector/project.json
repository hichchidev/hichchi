{
  "name": "nest-connector",
  "$schema": "/node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nest-connector/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": [
        "typedoc",
        {
          "projects": "dependencies",
          "target": "build"
        }
      ],
      "options": {
        "outputPath": "dist/libs/nest-connector",
        "tsConfig": "libs/nest-connector/tsconfig.lib.json",
        "packageJson": "libs/nest-connector/package.json",
        "main": "libs/nest-connector/src/index.ts",
        "assets": ["libs/nest-connector/*.md", "libs\\nest-connector\\docs"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "typedoc": {
      "executor": "nx:run-commands",
      "options": {
        "command": "typedoc --options ./libs/nest-connector/typedoc.json",
        "outputPath": ["dist\\docs\\libs\\nest-connector"]
      }
    }
  }
}
