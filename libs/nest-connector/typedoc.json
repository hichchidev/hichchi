{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts", "./src/auth.ts", "./src/crud.ts"],
  "out": "./",
  "tsconfig": "./tsconfig.json",
  "useCodeBlocks": true,
  "expandObjects": false,
  "expandParameters": false,
  "membersWithOwnFile": [],
  "mergeReadme": true,
  "readme": "./readme-top.md",
  "cleanOutputDir": false,
  "parametersFormat": "htmlTable",
  "interfacePropertiesFormat": "htmlTable",
  "classPropertiesFormat": "htmlTable",
  "enumMembersFormat": "htmlTable",
  "propertyMembersFormat": "htmlTable",
  "typeDeclarationFormat": "htmlTable",
  "formatWithPrettier": true,
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-remark"],
  "remarkPlugins": [
    ["remark-insert-headings", { "text": "ðŸ“‹ API Table of Contents" }],
    ["remark-toc", { "heading": "ðŸ“‹ API Table of Contents", "maxDepth": 3 }]
  ],
  "groupOrder": [
    "Project",
    "Module",
    "Namespace",
    "Class",
    "Function",
    "Interface",
    "TypeAlias",
    "Variable",
    "Enumeration",
    "*"
  ]
}
